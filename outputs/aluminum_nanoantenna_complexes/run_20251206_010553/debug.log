  # m/s\nhbar = 1.054571817e-34  # JÂ·s\neV_to_J = 1.602176634e-19  # J/eV\n\n# Angular frequency array\nomega = 2 * np.pi * c / wavelengths_m  # rad/s\n\nprint(f"\\n=== {stage_id}: Material Property Validation ===")\nprint(f"Wavelength range: {wl_min}-{wl_max} nm ({n_points} points)")\nprint(f"Started at: {datetime.now().strftime(\'%Y-%m-%d %H:%M:%S\')}")\n\n# â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\n# HELPER FUNCTIONS\n# â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\n\ndef eps_to_nk(eps_real, eps_imag):\n    """Convert complex permittivity to n and k."""\n    eps_complex = eps_real + 1j * eps_imag\n    n_complex = np.sqrt(eps_complex)\n    n = np.real(n_complex)\n    k = np.imag(n_complex)\n    return n, k\n\ndef validate_array(arr, name):\n    """Check array for NaN/Inf and exit with error if found."""\n    if np.any(np.isnan(arr)):\n        print(f"ERROR: NaN values detected in {name}")\n        print(f"  Shape: {arr.shape}, NaN count: {np.sum(np.isnan(arr))}")\n        sys.exit(1)\n    if np.any(np.isinf(arr)):\n        print(f"ERROR: Inf values detected in {name}")\n        print(f"  Shape: {arr.shape}, Inf count: {np.sum(np.isinf(arr))}")\n        sys.exit(1)\n\ndef find_fwhm(wavelengths, spectrum):\n    """Find FWHM of a peak in spectrum."""\n    max_idx = np.argmax(spectrum)\n    max_val = spectrum[max_idx]\n    half_max = max_val / 2\n    \n    # Find left crossing\n    left_idx = max_idx\n    while left_idx > 0 and spectrum[left_idx] > half_max:\n        left_idx -= 1\n    \n    # Find right crossing\n    right_idx = max_idx\n    while right_idx < len(spectrum) - 1 and spectrum[right_idx] > half_max:\n        right_idx += 1\n    \n    # Interpolate for better accuracy\n    if left_idx > 0:\n        frac_left = (spectrum[left_idx] - half_max) / (spectrum[left_idx] - spectrum[left_idx-1])\n        wl_left = wavelengths[left_idx] - frac_left * (wavelengths[left_idx] - wavelengths[left_idx-1])\n    else:\n        wl_left = wavelengths[0]\n    \n    if right_idx < len(spectrum) - 1:\n        frac_right = (spectrum[right_idx-1] - half_max) / (spectrum[right_idx-1] - spectrum[right_idx])\n        wl_right = wavelengths[right_idx-1] + frac_right * (wavelengths[right_idx] - wavelengths[right_idx-1])\n    else:\n        wl_right = wavelengths[-1]\n    \n    fwhm_nm = wl_right - wl_left\n    peak_wl = wavelengths[max_idx]\n    \n    return fwhm_nm, peak_wl\n\n# â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\n# MATERIAL 1: ALUMINUM (Palik tabulated data)\n# â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\n\nprint("\\n--- Loading Aluminum (Palik) data ---")\n\nal_data_file = "materials/palik_al.csv"\n\ntry:\n    # Use pandas for robust CSV loading with headers\n    al_df = pd.read_csv(al_data_file)\n    print(f"  Loaded {len(al_df)} data points from {al_data_file}")\n    print(f"  Columns: {list(al_df.columns)}")\n    \n    # Extract columns - try common naming conventions\n    if \'wavelength_nm\' in al_df.columns:\n        al_wl_data = al_df[\'wavelength_nm\'].values\n    elif \'wavelength_um\' in al_df.columns:\n        al_wl_data = al_df[\'wavelength_um\'].values * 1000  # Convert Âµm to nm\n    else:\n        # Assume first column is wavelength\n        al_wl_data = al_df.iloc[:, 0].values\n        print(f"  Warning: Using first column as wavelength")\n    \n    if \'n\' in al_df.columns and \'k\' in al_df.columns:\n        al_n_data = al_df[\'n\'].values\n        al_k_data = al_df[\'k\'].values\n    elif \'eps_real\' in al_df.columns and \'eps_imag\' in al_df.columns:\n        # Convert from epsilon to n,k\n        al_eps_r = al_df[\'eps_r