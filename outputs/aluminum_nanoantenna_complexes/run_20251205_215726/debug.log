    if np.any(al_raw[:, 1] < 0):\n        # Data is eps_real, eps_imag\n        al_eps_real_raw = al_raw[:, 1]\n        al_eps_imag_raw = al_raw[:, 2]\n        al_n_raw, al_k_raw = eps_to_nk(al_eps_real_raw, al_eps_imag_raw)\n        print(f"  Detected format: wavelength, eps_real, eps_imag")\n    else:\n        # Data is n, k\n        al_n_raw = al_raw[:, 1]\n        al_k_raw = al_raw[:, 2]\n        # Convert n,k to epsilon\n        al_eps_real_raw = al_n_raw**2 - al_k_raw**2\n        al_eps_imag_raw = 2 * al_n_raw * al_k_raw\n        print(f"  Detected format: wavelength, n, k")\n    \n    # Sort by wavelength if needed\n    sort_idx = np.argsort(al_wl_nm_raw)\n    al_wl_nm_raw = al_wl_nm_raw[sort_idx]\n    al_eps_real_raw = al_eps_real_raw[sort_idx]\n    al_eps_imag_raw = al_eps_imag_raw[sort_idx]\n    al_n_raw = al_n_raw[sort_idx]\n    al_k_raw = al_k_raw[sort_idx]\n    \n    print(f"  Data range: {al_wl_nm_raw.min():.1f} - {al_wl_nm_raw.max():.1f} nm")\n    print(f"  Points loaded: {len(al_wl_nm_raw)}")\n    \n    # Interpolate to target wavelength array\n    # Only interpolate within data range\n    valid_mask = (wavelength_nm >= al_wl_nm_raw.min()) & (wavelength_nm <= al_wl_nm_raw.max())\n    \n    al_eps_real = np.interp(wavelength_nm, al_wl_nm_raw, al_eps_real_raw)\n    al_eps_imag = np.interp(wavelength_nm, al_wl_nm_raw, al_eps_imag_raw)\n    al_n, al_k = eps_to_nk(al_eps_real, al_eps_imag)\n    \n    # Set out-of-range values to NaN and report\n    if not np.all(valid_mask):\n        out_of_range = np.sum(~valid_mask)\n        print(f"  WARNING: {out_of_range} points outside data range - using extrapolation")\n    \n    print(f"  Îµ_real range: {al_eps_real.min():.1f} to {al_eps_real.max():.1f}")\n    print(f"  Îµ_imag range: {al_eps_imag.min():.1f} to {al_eps_imag.max():.1f}")\n    \n    # Check for interband transition around 800 nm (1.5 eV)\n    idx_800 = np.argmin(np.abs(wavelength_nm - 800))\n    idx_600 = np.argmin(np.abs(wavelength_nm - 600))\n    print(f"  Îµ_imag at 600nm: {al_eps_imag[idx_600]:.2f}")\n    print(f"  Îµ_imag at 800nm: {al_eps_imag[idx_800]:.2f}")\n    \n    al_loaded = True\nelse:\n    print(f"  ERROR: File not found: {al_path}")\n    print(f"  Creating dummy data for demonstration")\n    # Drude model approximation for Al\n    wp = 15.0  # eV (plasma frequency)\n    gamma_drude = 0.1  # eV (damping)\n    eps_inf_al = 1.0\n    al_eps_real = eps_inf_al - (wp**2) / (energy_eV**2 + gamma_drude**2)\n    al_eps_imag = (wp**2 * gamma_drude) / (energy_eV * (energy_eV**2 + gamma_drude**2))\n    al_n, al_k = eps_to_nk(al_eps_real, al_eps_imag)\n    al_loaded = False\n\nvalidate_array(al_eps_real, "Al eps_real")\nvalidate_array(al_eps_imag, "Al eps_imag")\n\n# â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\n# MATERIAL 2: TDBC J-AGGREGATE (Lorentzian Model)\n# â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\n\nprint("\\n--- Material 2: TDBC J-aggregate (Lorentzian) ---")\n\n# Paper Methods parameters (exact values stated)\neps_inf_tdbc = 2.56       # High-frequency permittivity\nomega_0 = 3.22e15         # Exciton resonance frequency (rad/s)\ngamma_methods = 2.45e13   # Linewidth from Methods (rad/s)\nf_osc = 0.45              # Oscillator strength\n\n# Convert to eV for display\nomega_0_eV = omega_to_eV(omega_0)\ngamma_methods_eV = omega_to_eV(gamma_methods)\nlambda_0_nm = 2 * np.pi * c / omega_0 * 1e9\n\nprint(f"Parameters from paper Methods:")\nprint(f"  Îµâˆ = {eps_inf_tdbc}")\nprint(f"  Ï‰â‚€ = {omega_0:.3e} rad/s = {omega_0_eV:.3f} eV")\nprint(f"  Î»â‚€ = {lambda_0_nm:.1f} nm")\nprint(f"  Î³ (Methods) = {gamma_methods:.3e} rad/s = {gamma_methods_eV:.4f} eV")\nprint(f"  f = {f_osc}")\n\n# Lorentzian model: Îµ(Ï‰) = Îµâˆ + fÂ·Ï‰â‚€Â²/(Ï‰â‚€Â² - Ï‰Â² - iÎ³Ï‰)\n# This is the standard form for an oscillator\n\ndef compute_lorentzian_eps(omega_array, eps_inf, omega_res, gamma, f):\n    """Compute Lorentzian permittivity."""\n    numerator = f * omega_res**2\n    denominator = omega_res**2 - omega_array**2 - 1j * gamma * omega_array\n    eps_complex = eps_inf + numerator / denominator\n    return np.real(eps_complex), np.imag(eps_complex)\n\n# Compute with Methods linewidth\ntdbc_eps_real_methods, tdbc_eps_imag_methods = compute_lorentzian_eps(\n    omega, eps_inf_tdbc, omega_0, gamma_methods, f_osc\n)\ntdbc_n_methods, tdbc_k_methods = eps_to_nk(tdbc_eps_real_methods, tdbc_eps_imag_methods)\n\n# Calculate absorption coefficient (relative)\n# Î± âˆ Ï‰Â·k/c, but for comparison just use k normalized\ntdbc_abs_methods = tdbc_k_methods * omega / c  # Proportional to absorption coefficient\n\n# Also compute with 0.066 eV linewidth mentioned in text (for comparison)\ngamma_text_eV = 0.066  # eV\ngamma_text = gamma_text_eV / hbar_eV  # Convert to rad/s\nprint(f"\\nAlternative linewidth from text:")\nprint(f"  Î³ (text) = {gamma_text_eV} eV = {gamma_text:.3e} rad/s")\nprint(f"  Ratio Î³_text/Î³_methods = {gamma_text/gamma_methods:.1f}x")\n\ntdbc_eps_real_text, tdbc_eps_imag_text = compute_lorentzian_eps(\n    omega, eps_inf_tdbc, omega_0, gamma_text, f_osc\n)\ntdbc_n_text, tdbc_k_text = eps_to_nk(tdbc_eps_real_text, tdbc_eps_imag_text)\ntdbc_abs_text = tdbc_k_text * omega / c\n\n# Find peak position and FWHM for Methods version\npeak_idx = np.argmax(tdbc_k_methods)\npeak_wavelength_nm = wavelength_nm[peak_idx]\npeak_k = tdbc_k_methods[peak_idx]\nhalf_max = peak_k / 2\n\n# Find FWHM\nleft_idx = np.argmin(np.abs(tdbc_k_methods[:peak_idx] - half_max))\nright_idx = peak_idx + np.argmin(np.abs(tdbc_k_methods[peak_idx:] - half_max))\nfwhm_nm_methods = wavelength_nm[left_idx] - wavelength_nm[right_idx]\n\nprint(f"\\nMethods linewidth TDBC spectrum:")\nprint(f"  Peak wavelength: {peak_wavelength_nm:.1f} nm ({omega_to_eV(wavelength_to_omega(peak_wavelength_nm)):.3f} eV)")\nprint(f"  FWHM: ~{abs(fwhm_nm_methods):.1f} nm")\nprint(f"  Peak k: {peak_k:.3f}")\n\n# Do the same for text version\npeak_idx_text = np.argmax(tdbc_k_text)\npeak_k_text = tdbc_k_text[peak_idx_text]\nhalf_max_text = peak_k_text / 2\nleft_idx_text = np.argmin(np.abs(tdbc_k_text[:peak_idx_text] - half_max_text))\nright_idx_text = peak_idx_text + np.argmin(np.abs(tdbc_k_text[peak_idx_text:] - half_max_text))\nfwhm_nm_text = wavelength_nm[left_idx_text] - wavelength_nm[right_idx_text]\n\nprint(f"\\nText linewidth (0.066 eV) TDBC spectrum:")\nprint(f"  Peak wavelength: {wavelength_nm[peak_idx_text]:.1f} nm")\nprint(f"  FWHM: ~{abs(fwhm_nm_text):.1f} nm")\nprint(f"  Peak k: {peak_k_text:.3f}")\n\nvalidate_array(tdbc_eps_real_methods, "TDBC eps_real (Methods)")\nvalidate_array(tdbc_eps_imag_methods, "TDBC eps_imag (Methods)")\n\n# Use Methods parameters as primary (they are explicit)\ntdbc_eps_real = tdbc_eps_real_methods\ntdbc_eps_imag = tdbc_eps_imag_methods\ntdbc_n = tdbc_n_methods\ntdbc_k = tdbc_k_methods\ntdbc_abs = tdbc_abs_methods\n\n# â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\n# MATERIAL 3: ITO (Konig Tabulated Data)\n# â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\n\nprint("\\n--- Material 3: ITO (Konig) ---")\n\n# Resolve path from material_id\nito_entry = mat_lookup.get(\'konig_ito\', {})\nito_data_file = ito_entry.get(\'data_file\')\nito_path = f"materials/{ito_data_file}" if ito_data_file else "materials/konig_ito.csv"\n\nprint(f"Data file: {ito_path}")\n\nif os.path.exists(ito_path):\n    # Load ITO data\n    ito_raw = np.loadtxt(ito_path, delimiter=\',\', skiprows=1)\n    \n    # Check column format\n    if ito_raw[0, 0] < 10:  # micrometers\n        ito_wl_nm_raw = ito_raw[:, 0] * 1000\n        print(f"  Detected wavelength in Î¼m, converting to nm")\n    else:\n        ito_wl_nm_raw = ito_raw[:, 0]\n    \n    # ITO is a dielectric - expect positive eps_real or n,k format\n    if np.all(ito_raw[:, 1] > 0) and np.all(ito_raw[:, 1] < 10):\n        # Likely n, k format\n        ito_n_raw = ito_raw[:, 1]\n        ito_k_raw = ito_raw[:, 2] if ito_raw.shape[1] > 2 else np.zeros_like(ito_n_raw)\n        ito_eps_real_raw = ito_n_raw**2 - ito_k_raw**2\n        ito_eps_imag_raw = 2 * ito_n_raw * ito_k_raw\n        print(f"  Detected format: wavelength, n, k")\n    else:\n        # eps_real, eps_imag format\n        ito_eps_real_raw = ito_raw[:, 1]\n        ito_eps_imag_raw = ito_raw[:, 2]\n        ito_n_raw, ito_k_raw = eps_to_nk(ito_eps_real_raw, ito_eps_imag_raw)\n        print(f"  Detected format: wavelength, eps_real, eps_imag")\n    \n    # Sort by wavelength\n    sort_idx = np.argsort(ito_wl_nm_raw)\n    ito_wl_nm_raw = ito_wl_nm_raw[sort_idx]\n    ito_eps_real_raw = ito_eps_real_raw[sort_idx]\n    ito_eps_imag_raw = ito_eps_imag_raw[sort_idx]\n    ito_n_raw = ito_n_raw[sort_idx]\n    ito_k_raw = ito_k_raw[sort_idx]\n    \n    print(f"  Data range: {ito_wl_nm_raw.min():.1f} - {ito_wl_nm_raw.max():.1f} nm")\n    print(f"  Points loaded: {len(ito_wl_nm_raw)}")\n    \n    # Interpolate to target wavelength array\n    ito_eps_real = np.interp(wavelength_nm, ito_wl_nm_raw, ito_eps_real_raw)\n    ito_eps_imag = np.interp(wavelength_nm, ito_wl_nm_raw, ito_eps_imag_raw)\n    ito_n, ito_k = eps_to_nk(ito_eps_real, ito_eps_imag)\n    \n    print(f"  n range: {ito_n.min():.2f} to {ito_n.max():.2f}")\n    print(f"  k range: {ito_k.min():.4f} to {ito_k.max():.4f}")\n    \n    ito_loaded = True\nelse:\n    print(f"  ERROR: File not found: {ito_path}")\n    print(f"  Creating typical ITO approximation")\n    # Typical ITO values in visible range\n    ito_n = np.ones_like(wavelength_nm) * 1.9\n    ito_k = np.ones_like(wavelength_nm) * 0.02\n    ito_eps_real = ito_n**2 - ito_k**2\n    ito_eps_imag = 2 * ito_n * ito_k\n    ito_loaded = False\n\nvalidate_array(ito_eps_real, "ITO eps_real")\nvalidate_array(ito_eps_imag, "ITO eps_imag")\n\n# â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\n# MATERIAL 4: GLASS (Constant)\n# â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\n\nprint("\\n--- Material 4: Glass (Constant) ---")\n\nn_glass = 1.51\neps_glass = n_glass**2\nprint(f"  n = {n_glass}")\nprint(f"  Îµ = {eps_glass}")\n\n# â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\n# SAVE CSV DATA FILES\n# â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\n\nprint("\\n--- Saving Data Files ---")\n\n# Aluminum data\nal_header = f"""# Stage: stage0_material_validation\n# Material: Aluminum (Palik)\n# Source: Palik Handbook tabulated data\n# Generated: {datetime.now().isoformat()}\n# Wavelength range: 400-900 nm\n# Columns: wavelength_nm, eps_real, eps_imag, n, k\n"""\nal_data = np.column_stack([wavelength_nm, al_eps_real, al_eps_imag, al_n, al_k])\nnp.savetxt(\'stage0_aluminum_eps_palik.csv\', al_data, \n           header=al_header.strip(), delimiter=\',\', fmt=\'%.6f\', comments=\'\')\nprint("  Saved: stage0_aluminum_eps_palik.csv")\n\n# TDBC data (using Methods parameters)\ntdbc_header = f"""# Stage: stage0_material_validation\n# Material: TDBC J-aggregate (Lorentzian)\n# Source: Paper Methods section\n# Parameters: eps_inf={eps_inf_tdbc}, omega_0={omega_0:.3e} rad/s, gamma={gamma_methods:.3e} rad/s, f={f_osc}\n# Generated: {datetime.now().isoformat()}\n# Wavelength range: 400-900 nm\n# Columns: wavelength_nm, eps_real, eps_imag, n, k, absorption_coeff\n"""\ntdbc_data = np.column_stack([wavelength_nm, tdbc_eps_real, tdbc_eps_imag, tdbc_n, tdbc_k, tdbc_abs])\nnp.savetxt(\'stage0_tdbc_eps_lorentzian.csv\', tdbc_data,\n           header=tdbc_header.strip(), delimiter=\',\', fmt=\'%.6f\', comments=\'\')\nprint("  Saved: stage0_tdbc_eps_lorentzian.csv")\n\n# ITO data\nito_header = f"""# Stage: stage0_material_validation\n# Material: ITO (Konig)\n# Source: Konig tabulated data\n# Generated: {datetime.now().isoformat()}\n# Wavelength range: 400-900 nm\n# Columns: wavelength_nm, eps_real, eps_imag, n, k\n"""\nito_data = np.column_stack([wavelength_nm, ito_eps_real, ito_eps_imag, ito_n, ito_k])\nnp.savetxt(\'stage0_ito_eps_konig.csv\', ito_data,\n           header=ito_header.strip(), delimiter=\',\', fmt=\'%.6f\', comments=\'\')\nprint("  Saved: stage0_ito_eps_konig.csv")\n\n# â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\n# PLOT 1: COMBINED MATERIALS VALIDATION\n# â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\n\nprint("\\n--- Generating Validation Plots ---")\n\nfig, axes = plt.subplots(3, 3, figsize=(14, 12))\n\n# Row 1: Aluminum\nax = axes[0, 0]\nax.plot(wavelength_nm, al_eps_real, \'b-\', linewidth=1.5)\nax.set_xlabel(\'Wavelength (nm)\')\nax.set_ylabel(\'Îµâ‚ (real)\')\nax.set_title(\'Aluminum (Palik) - Îµ real\')\nax.grid(True, alpha=0.3)\nax.axhline(y=0, color=\'k\', linestyle=\'--\', alpha=0.3)\n\nax = axes[0, 1]\nax.plot(wavelength_nm, al_eps_imag, \'r-\', linewidth=1.5)\nax.set_xlabel(\'Wavelength (nm)\')\nax.set_ylabel(\'Îµâ‚‚ (imag)\')\nax.set_title(\'Aluminum (Palik) - Îµ imag\')\nax.grid(True, alpha=0.3)\n# Mark interband transition region\nax.axvline(x=800, color=\'g\', linestyle=\'--\', alpha=0.5, label=\'~800nm (1.5 eV)\')\nax.legend()\n\nax = axes[0, 2]\nax.plot(wavelength_nm, al_n, \'b-\', label=\'n\', linewidth=1.5)\nax.plot(wavelength_nm, al_k, \'r-\', label=\'k\', linewidth=1.5)\nax.set_xlabel(\'Wavelength (nm)\')\nax.set_ylabel(\'n, k\')\nax.set_title(\'Aluminum (Palik) - n, k\')\nax.legend()\nax.grid(True, alpha=0.3)\n\n# Row 2: TDBC\nax = axes[1, 0]\nax.plot(wavelength_nm, tdbc_eps_real, \'b-\', linewidth=1.5, label=f\'Î³={gamma_methods_eV:.4f}eV (Methods)\')\nax.plot(wavelength_nm, tdbc_eps_real_text, \'b--\', linewidth=1.0, alpha=0.7, label=f\'Î³={gamma_text_eV}eV (Text)\')\nax.set_xlabel(\'Wavelength (nm)\')\nax.set_ylabel(\'Îµâ‚ (real)\')\nax.set_title(\'TDBC J-aggregate - Îµ real\')\nax.legend(fontsize=8)\nax.grid(True, alpha=0.3)\nax.axvline(x=lambda_0_nm, color=\'g\', linestyle=\'--\', alpha=0.5)\n\nax = axes[1, 1]\nax.plot(wavelength_nm, tdbc_eps_imag, \'r-\', linewidth=1.5, label=f\'Î³={gamma_methods_eV:.4f}eV (Methods)\')\nax.plot(wavelength_nm, tdbc_eps_imag_text, \'r--\', linewidth=1.0, alpha=0.7, label=f\'Î³={gamma_text_eV}eV (Text)\')\nax.set_xlabel(\'Wavelength (nm)\')\nax.set_ylabel(\'Îµâ‚‚ (imag)\')\nax.set_title(\'TDBC J-aggregate - Îµ imag\')\nax.legend(fontsize=8)\nax.grid(True, alpha=0.3)\nax.axvline(x=lambda_0_nm, color=\'g\', linestyle=\'--\', alpha=0.5)\n\nax = axes[1, 2]\nax.plot(wavelength_nm, tdbc_n, \'b-\', label=\'n (Methods)\', linewidth=1.5)\nax.plot(wavelength_nm, tdbc_k, \'r-\', label=\'k (Methods)\', linewidth=1.5)\nax.plot(wavelength_nm, tdbc_k_text, \'r--\', label=\'k (Text)\', linewidth=1.0, alpha=0.7)\nax.set_xlabel(\'Wavelength (nm)\')\nax.set_ylabel(\'n, k\')\nax.set_title(\'TDBC J-aggregate - n, k\')\nax.legend(fontsize=8)\nax.grid(True, alpha=0.3)\nax.axvline(x=lambda_0_nm, color=\'g\', linestyle=\'--\', alpha=0.5)\n\n# Row 3: ITO and Glass\nax = axes[2, 0]\nax.plot(wavelength_nm, ito_eps_real, \'b-\', linewidth=1.5)\nax.set_xlabel(\'Wavelength (nm)\')\nax.set_ylabel(\'Îµâ‚ (real)\')\nax.set_title(\'ITO (Konig) - Îµ real\')\nax.grid(True, alpha=0.3)\n\nax = axes[2, 1]\nax.plot(wavelength_nm, ito_eps_imag, \'r-\', linewidth=1.5)\nax.set_xlabel(\'Wavelength (nm)\')\nax.set_ylabel(\'Îµâ‚‚ (imag)\')\nax.set_title(\'ITO (Konig) - Îµ imag\')\nax.grid(True, alpha=0.3)\n\nax = axes[2, 2]\nax.plot(wavelength_nm, ito_n, \'b-\', label=\'n (ITO)\', linewidth=1.5)\nax.plot(wavelength_nm, ito_k, \'r-\', label=\'k (ITO)\', linewidth=1.5)\nax.axhline(y=n_glass, color=\'purple\', linestyle=\'--\', label=f\'n (Glass) = {n_glass}\')\nax.set_xlabel(\'Wavelength (nm)\')\nax.set_ylabel(\'n, k\')\nax.set_title(\'ITO & Glass - n, k\')\nax.legend(fontsize=8)\nax.grid(True, alpha=0.3)\n\nplt.suptitle(\'Stage 0: Material Validation - Target: Fig 2a\', fontsize=14, fontweight=\'bold\')\nplt.tight_layout()\nplt.savefig(\'stage0_materials_validation.png\', dpi=200, bbox_inches=\'tight\')\nplt.close()\nprint("  Saved: stage0_materials_validation.png")\n\n# â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\n# PLOT 2: TDBC ABSORPTION DETAIL\n# â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\n\nfig, axes = plt.subplots(1, 2, figsize=(12, 5))\n\n# Plot 1: Absorption spectrum comparison\nax = axes[0]\n# Normalize to peak for comparison\nax.plot(wavelength_nm, tdbc_k / tdbc_k.max(), \'b-\', linewidth=2, \n        label=f\'Methods: Î³={gamma_methods_eV:.4f}eV, FWHMâ‰ˆ{abs(fwhm_nm_methods):.0f}nm\')\nax.plot(wavelength_nm, tdbc_k_text / tdbc_k_text.max(), \'r--\', linewidth=2, \n        label=f\'Text: Î³={gamma_text_eV}eV, FWHMâ‰ˆ{abs(fwhm_nm_text):.0f}nm\')\nax.axvline(x=lambda_0_nm, color=\'g\', linestyle=\':\', alpha=0.7, label=f\'Î»â‚€={lambda_0_nm:.0f}nm\')\nax.axvline(x=590, color=\'orange\', linestyle=\':\', alpha=0.7, label=\'590nm (2.1eV)\')\nax.set_xlabel(\'Wavelength (nm)\', fontsize=12)\nax.set_ylabel(\'Normalized extinction (k)\', fontsize=12)\nax.set_title(\'TDBC Absorption: Methods vs Text Linewidth\', fontsize=12)\nax.legend(fontsize=9)\nax.grid(True, alpha=0.3)\nax.set_xlim(500, 700)\n\n# Plot 2: Permittivity near resonance\nax = axes[1]\nwl_