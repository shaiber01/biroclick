â•\n\nprint("\\n" + "=" * 60)\nprint("PART 2: TDBC J-AGGREGATE (Lorentzian) VALIDATION")\nprint("=" * 60)\n\n# Paper-stated Lorentzian parameters\neps_inf = 2.56  # Background permittivity\nomega_X = 3.22e15  # rad/s - resonance frequency\ngamma_X = 2.45e13  # rad/s - damping rate\nf_osc = 0.45  # Oscillator strength\n\n# Convert to eV for reference\nomega_X_eV = omega_rad_s_to_eV(omega_X)\ngamma_X_eV = omega_rad_s_to_eV(gamma_X)\n\nprint(f"\\nPaper-stated Lorentzian parameters:")\nprint(f"  Îµâˆ = {eps_inf}")\nprint(f"  Ï‰X = {omega_X:.3e} rad/s = {omega_X_eV:.4f} eV")\nprint(f"  Î³X = {gamma_X:.3e} rad/s = {gamma_X_eV:.4f} eV")\nprint(f"  f  = {f_osc}")\n\n# Expected target values from paper Fig 2a\ntarget_peak_eV = 2.1  # eV\ntarget_linewidth_eV = 0.066  # eV\n\nprint(f"\\nTarget values (Fig 2a):")\nprint(f"  Peak energy: {target_peak_eV} eV")\nprint(f"  Linewidth (FWHM): {target_linewidth_eV} eV")\n\n# Create energy/wavelength array for TDBC calculation\ntdbc_energy_eV = np.linspace(1.5, 2.8, 401)\ntdbc_wl_nm = eV_to_wavelength_nm(tdbc_energy_eV)\ntdbc_omega = eV_to_omega_rad_s(tdbc_energy_eV)\n\n# Compute Lorentzian dielectric function\n# Standard form: Îµ(Ï‰) = Îµâˆ + fÂ·Ï‰â‚€Â² / (Ï‰â‚€Â² - Ï‰Â² - iÂ·Î³Â·Ï‰)\nomega_0_sq = omega_X ** 2\nnumerator = f_osc * omega_0_sq\ndenominator = omega_0_sq - tdbc_omega**2 - 1j * gamma_X * tdbc_omega\n\ntdbc_eps = eps_inf + numerator / denominator\ntdbc_eps_real = tdbc_eps.real\ntdbc_eps_imag = tdbc_eps.imag\n\n# Compute complex refractive index: n_complex = sqrt(Îµ)\ntdbc_n_complex = np.sqrt(tdbc_eps)\ntdbc_n = tdbc_n_complex.real\ntdbc_k = tdbc_n_complex.imag\n\n# Compute absorption (proportional to Im(Îµ) for thin film, or Ï‰Â·k for bulk)\n# Using Im(Îµ) which captures peak position well\ntdbc_absorption = tdbc_eps_imag / tdbc_eps_imag.max()  # Normalized\n\n# Validate arrays\nvalidate_array(tdbc_eps_real, "TDBC eps_real")\nvalidate_array(tdbc_eps_imag, "TDBC eps_imag")\nvalidate_array(tdbc_n, "TDBC n")\nvalidate_array(tdbc_k, "TDBC k")\n\n# â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\n# TDBC VALIDATION ANALYSIS\n# â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\n\nprint("\\n--- TDBC Validation Analysis ---")\n\n# Find peak of Im(Îµ)\npeak_idx = np.argmax(tdbc_eps_imag)\npeak_energy = tdbc_energy_eV[peak_idx]\npeak_wavelength = tdbc_wl_nm[peak_idx]\nprint(f"\\nPeak of Im(Îµ):")\nprint(f"  Energy: {peak_energy:.4f} eV (target: {target_peak_eV} eV)")\nprint(f"  Wavelength: {peak_wavelength:.1f} nm")\nprint(f"  Peak error: {abs(peak_energy - target_peak_eV)/target_peak_eV * 100:.1f}%")\n\n# Compute FWHM of absorption peak\nhalf_max = tdbc_eps_imag[peak_idx] / 2\nabove_half = tdbc_eps_imag >= half_max\ntransitions = np.diff(above_half.astype(int))\nrise_idx = np.where(transitions == 1)[0]\nfall_idx = np.where(transitions == -1)[0]\n\nif len(rise_idx) > 0 and len(fall_idx) > 0:\n    # Interpolate for more accurate FWHM\n    E_low = tdbc_energy_eV[rise_idx[0]]\n    E_high = tdbc_energy_eV[fall_idx[0]]\n    measured_fwhm_eV = E_high - E_low\nelse:\n    # Fallback: estimate from Î³\n    measured_fwhm_eV = 2 * gamma_X_eV  # FWHM â‰ˆ 2*HWHM for Lorentzian\n\nprint(f"\\nLinewidth (FWHM) analysis:")\nprint(f"  Measured FWHM from Im(Îµ): {measured_fwhm_eV:.4f} eV")\nprint(f"  Expected from Î³X (2Ã—HWHM): {2*gamma_X_eV:.4f} eV")\nprint(f"  Target from paper: {target_linewidth_eV:.4f} eV")\nprint(f"  Ratio (measured/target): {measured_fwhm_eV/target_linewidth_eV:.2f}")\n\n# CRITICAL DISCREPANCY ANALYSIS\nprint("\\n" + "=" * 40)\nprint("CRITICAL: LINEWIDTH DISCREPANCY ANALYSIS")\nprint("=" * 40)\ndiscrepancy_ratio = target_linewidth_eV / measured_fwhm_eV\nprint(f"Paper linewidth / computed linewidth = {discrepancy_ratio:.2f}")\n\nif discrepancy_ratio > 1.5:\n    print("\\n*** DISCREPANCY DETECTED ***")\n    print(f"Paper states ~{target_linewidth_eV} eV linewidth")\n    print(f"But Î³X={gamma_X_eV:.4f} eV gives FWHM â‰ˆ {measured_fwhm_eV:.4f} eV")\n    print("\\nPossible explanations:")\n    print("  1. Paper\'s Î³X may define FWHM (not HWHM)")\n    print("  2. Additional broadening mechanisms not in simple Lorentzian")\n    print("  3. Different linewidth definition (e.g., 1/e width)")\n    print("\\nRecommendation: Verify with user before proceeding")\n    \n    # Also compute what Î³ would need to be for target linewidth\n    required_gamma_eV = target_linewidth_eV / 2  # HWHM\n    required_gamma_rad_s = eV_to_omega_rad_s(required_gamma_eV)\n    print(f"\\nTo match target linewidth:")\n    print(f"  Required Î³ (HWHM): {required_gamma_eV:.4f} eV")\n    print(f"  Required Î³: {required_gamma_rad_s:.3e} rad/s")\n    print(f"  This is {required_gamma_rad_s/gamma_X:.2f}Ã— the paper\'s Î³X value")\n\n# Validation status\npeak_error_pct = abs(peak_energy - target_peak_eV) / target_peak_eV * 100\nlinewidth_error_pct = abs(measured_fwhm_eV - target_linewidth_eV) / target_linewidth_eV * 100\n\ntdbc_peak_valid = peak_error_pct < 5  # Within 5%\ntdbc_linewidth_valid = linewidth_error_pct < 30  # Within 30%\n\nprint(f"\\n--- TDBC Validation Status ---")\nprint(f"Peak position: {\'PASS\' if tdbc_peak_valid else \'FAIL\'} ({peak_error_pct:.1f}% error)")\nprint(f"Linewidth: {\'PASS\' if tdbc_linewidth_valid else \'INVESTIGATE\'} ({linewidth_error_pct:.1f}% error)")\n\n# â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\n# SAVE TDBC DATA\n# â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\n\ntdbc_output_data = np.column_stack([\n    tdbc_wl_nm, tdbc_energy_eV, tdbc_eps_real, tdbc_eps_imag, tdbc_n, tdbc_k, tdbc_absorption\n])\ntdbc_header = """# Stage 0: TDBC J-aggregate Lorentzian dielectric function\n# Parameters: eps_inf={}, omega_X={} rad/s, gamma_X={} rad/s, f={}\n# Generated: {}\n# Columns: wavelength_nm, energy_eV, eps_real, eps_imag, n, k, absorption_au\nwavelength_nm,energy_eV,eps_real,eps_imag,n,k,absorption_au""".format(\n    eps_inf, omega_X, gamma_X, f_osc, datetime.now().isoformat())\n\nnp.savetxt(\'stage0_TDBC_lorentzian.csv\', tdbc_output_data,\n           header=tdbc_header, delimiter=\',\', fmt=\'%.6f\', comments=\'\')\nprint("\\nSaved: stage0_TDBC_lorentzian.csv")\n\n# â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\n# PLOT TDBC ABSORPTION (Compare to Fig 2a)\n# â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\n\nfig, axes = plt.subplots(2, 2, figsize=(12, 10))\n\n# Absorption spectrum vs energy\nax1 = axes[0, 0]\nax1.plot(tdbc_energy_eV, tdbc_absorption, \'b-\', linewidth=2, label=\'Computed\')\nax1.axvline(x=target_peak_eV, color=\'r\', linestyle=\'--\', alpha=0.7, label=f\'Target peak: {target_peak_eV} eV\')\nax1.axvline(x=peak_energy, color=\'g\', linestyle=\':\', alpha=0.7, label=f\'Computed peak: {peak_energy:.3f} eV\')\nax1.set_xlabel(\'Energy (eV)\')\nax1.set_ylabel(\'Absorption (normalized)\')\nax1.set_title(\'TDBC Absorption Spectrum â€“ Target: Fig 2a\')\nax1.legend()\nax1.grid(True, alpha=0.3)\nax1.set_xlim(1.8, 2.5)\n\n# Im(Îµ) spectrum\nax2 = axes[0, 1]\nax2.plot(tdbc_energy_eV, tdbc_eps_imag, \'r-\', linewidth=2)\nax2.axvline(x=target_peak_eV, color=\'k\', linestyle=\'--\', alpha=0.5)\nax2.set_xlabel(\'Energy (eV)\')\nax2.set_ylabel(\'Im(Îµ)\')\nax2.set_title(\'TDBC: Imaginary Permittivity\')\nax2.grid(True, alpha=0.3)\nax2.set_xlim(1.8, 2.5)\n\n# Re(Îµ) spectrum\nax3 = axes[1, 0]\nax3.plot(tdbc_energy_eV, tdbc_eps_real, \'g-\', linewidth=2)\nax3.axhline(y=0, color=\'k\', linestyle=\'--\', alpha=0.3)\nax3.axvline(x=target_peak_eV, color=\'k\