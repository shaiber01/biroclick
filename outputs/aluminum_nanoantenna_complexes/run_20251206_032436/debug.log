rmittivity Îµ = Îµâ‚ + iÎµâ‚‚"""\n    # n + ik = sqrt(Îµâ‚ + iÎµâ‚‚)\n    eps_complex = eps_real + 1j * eps_imag\n    n_complex = np.sqrt(eps_complex)\n    return np.real(n_complex), np.imag(n_complex)\n\ndef compute_epsilon_from_n_k(n, k):\n    """Compute Îµ from n and k: Îµ = (n + ik)Â²"""\n    eps_real = n**2 - k**2\n    eps_imag = 2 * n * k\n    return eps_real, eps_imag\n\ndef find_fwhm(x, y):\n    """Find FWHM of a peak. Returns FWHM and peak position."""\n    # Find peak\n    peak_idx = np.argmax(y)\n    peak_x = x[peak_idx]\n    peak_y = y[peak_idx]\n    half_max = peak_y / 2.0\n    \n    # Find left half-max crossing\n    left_idx = peak_idx\n    while left_idx > 0 and y[left_idx] > half_max:\n        left_idx -= 1\n    if left_idx > 0:\n        # Linear interpolation\n        x1, x2 = x[left_idx], x[left_idx + 1]\n        y1, y2 = y[left_idx], y[left_idx + 1]\n        left_x = x1 + (half_max - y1) * (x2 - x1) / (y2 - y1)\n    else:\n        left_x = x[0]\n    \n    # Find right half-max crossing\n    right_idx = peak_idx\n    while right_idx < len(y) - 1 and y[right_idx] > half_max:\n        right_idx += 1\n    if right_idx < len(y) - 1:\n        # Linear interpolation\n        x1, x2 = x[right_idx - 1], x[right_idx]\n        y1, y2 = y[right_idx - 1], y[right_idx]\n        right_x = x1 + (half_max - y1) * (x2 - x1) / (y2 - y1)\n    else:\n        right_x = x[-1]\n    \n    fwhm = abs(right_x - left_x)\n    return fwhm, peak_x\n\ndef validate_array(arr, name):\n    """Check array for NaN/Inf and exit with error if found."""\n    if np.any(np.isnan(arr)):\n        print(f"ERROR: NaN values detected in {name}")\n        print(f"  Shape: {arr.shape}, NaN count: {np.sum(np.isnan(arr))}")\n        sys.exit(1)\n    if np.any(np.isinf(arr)):\n        print(f"ERROR: Inf values detected in {name}")\n        print(f"  Shape: {arr.shape}, Inf count: {np.sum(np.isinf(arr))}")\n        sys.exit(1)\n\n# â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â