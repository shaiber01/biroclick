 + frac_left * (energy_eV[left_idx+1] - energy_eV[left_idx])\nelse:\n    energy_left = energy_eV[left_idx]\n\nif right_idx < len(energy_eV) - 1:\n    frac_right = (half_max - tdbc_eps_imag[right_idx]) / (tdbc_eps_imag[right_idx-1] - tdbc_eps_imag[right_idx])\n    energy_right = energy_eV[right_idx] - frac_right * (energy_eV[right_idx] - energy_eV[right_idx-1])\nelse:\n    energy_right = energy_eV[right_idx]\n\ncomputed_fwhm_eV = abs(energy_left - energy_right)\n\nprint(f"\\nLinewidth (FWHM):")\nprint(f"  Computed from curve = {computed_fwhm_eV:.4f} eV")\nprint(f"  Target (paper) = {paper_fwhm_eV} eV")\n\n# Calculate errors\npeak_error_pct = abs(peak_energy_eV - 2.1) / 2.1 * 100\nfwhm_error_pct = abs(computed_fwhm_eV - paper_fwhm_eV) / paper_fwhm_eV * 100\n\nprint(f"\\nValidation results:")\nprint(f"  Peak position error: {peak_error_pct:.1f}% (acceptable: <5%)")\nprint(f"  FWHM error: {fwhm_error_pct:.1f}% (acceptable: <30%)")\n\npeak_ok = peak_error_pct < 5\nfwhm_ok = fwhm_error_pct < 30\n\n# â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\n# VALIDATE DATA FOR NaN/Inf BEFORE SAVING\n# â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\n\ndef validate_array(arr, name):\n    """Check array for NaN/Inf and exit with error if found."""\n    if np.any(np.isnan(arr)):\n        print(f"ERROR: NaN values detected in {name}")\n        print(f"  Shape: {arr.shape}, NaN count: {np.sum(np.isnan(arr))}")\n        sys.exit(1)\n    if np.any(np.isinf(arr)):\n        print(f"ERROR: Inf values detected in {name}")\n        print(f"  Shape: {arr.shape}, Inf count: {np.sum(np.isinf(arr))}")\n        sys.exit(1)\n\nprint("\\n--- Validating computed data ---")\nvalidate_array(al_n_interp, "Al n")\nvalidate_array(al_k_interp, "Al k")\nvalidate_array(al_eps_real, "Al eps_real")\nvalidate_array(al_eps_imag, "Al eps_imag")\nvalidate_array(tdbc_eps_real, "TDBC eps_real")\nvalidate_array(tdbc_eps_imag, "TDBC eps_imag")\nvalidate_array(tdbc_n, "TDBC n")\nvalidate_array(tdbc_k, "TDBC k")\nprint("âœ“ All data validated (no NaN/Inf)")\n\n# â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\n# SAVE DATA FILES\n# â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\n\nprint("\\n--- Saving data files ---")\n\n# Aluminum optical constants\nal_header = f"""# Stage 0: Aluminum optical constants (Palik)\n# Generated: {datetime.now().isoformat()}\n# Source: {\'Palik tabulated data\' if al_loaded else \'Drude model fallback\'}\n# Wavelength range: {wl_min_nm}-{wl_max_nm} nm\n# Columns: wavelength_nm, energy_eV, n, k, eps_real, eps_imag\n"""\n\nal_data_out = np.column_stack([wavelength_nm, energy_eV, al_n_interp, al_k_interp, al_eps_real, al_eps_imag])\nnp.savetxt(\'stage0_aluminum_optical_constants.csv\', al_data_out, \n           header=al_header, delimiter=\',\', fmt=\'%.6f\', comments=\'\')\nprint("âœ“ Saved: stage0_aluminum_optical_constants.csv")\n\n# TDBC Lorentzian\ntdbc_header = f"""# Stage 0: TDBC J-aggregate Lorentzian dielectric function\n# Generated: {datetime.now().isoformat()}\n# Parameters: eps_inf={eps_inf_tdbc}, omega_0={omega_0_eV:.3f} eV, gamma={gamma_eV:.4f} eV (FWHM), f={f_osc}\n# NOTE: gamma back-calculated from paper\'s stated FWHM=0.066 eV\n# Columns: wavelength_nm, energy_eV, eps_real, eps_imag, n, k, absorption_au\n"""\n\ntdbc_data_out = np.column_stack([wavelength_nm, energy_eV, tdbc_eps_real, tdbc_eps_imag, \n                                  tdbc_n, tdbc_k, tdbc_absorption_norm])\nnp.savetxt(\'stage0_TDBC_lorentzian.csv\', tdbc_data_out,\n           header=tdbc_header, delimiter=\',\', fmt=\'%.6f\', comments=\'\')\nprint("âœ“ Saved: stage0_TDBC_lorentzian.csv")\n\n# â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\n# PLOT 1: ALUMINUM OPTICAL CONSTANTS\n# â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\n\nprint("\\n--- Generating plots ---")\n\nfig, axes = plt.subplots(2, 2, figsize=(12, 10))\n\n# n vs wavelength\nax1 = axes[0, 0]\nax1.plot(wavelength_nm, al_n_interp, \'b-\', linewidth=2)\nax1.set_xlabel(\'Wavelength (nm)\')\nax1.set_ylabel(\'n (refractive index)\')\nax1.set_title(\'Aluminum - Refractive Index n\')\nax1.grid(True, alpha=0.3)\nax1.set_xlim(wl_min_nm, wl_max_nm)\n\n# k vs wavelength\nax2 = axes[0, 1]\nax2.plot(wavelength_nm, al_k_interp, \'r-\', linewidth=2)\nax2.set_xlabel(\'Wavelength (nm)\')\nax2.set_ylabel(\'k (extinction coefficient)\')\nax2.set_title(\'Aluminum - Extinction Coefficient k\')\nax2.grid(True, alpha=0.3)\nax2.set_xlim(wl_min_nm, wl_max_nm)\n\n# Îµ_real vs wavelength\nax3 = axes[1, 0]\nax3.plot(wavelength_nm, al_eps_real, \'g-\', linewidth=2)\nax3.axhline(y=0, color=\'k\', linestyle=\'--\', alpha=0.5)\nax3.axvline(x=800, color=\'orange\', linestyle=\':\', alpha=0.7, label=\'Interband ~800nm\')\nax3.set_xlabel(\'Wavelength (nm)\')\nax3.set_ylabel(\'Îµ_real\')\nax3.set_title(\'Aluminum - Real Permittivity (Re(Îµ) < 0 = plasmonic)\')\nax3.grid(True, alpha=0.3)\nax3.set_xlim(wl_min_nm, wl_max_nm)\nax3.legend()\n\n# Îµ_imag vs wavelength\nax4 = axes[1, 1]\nax4.plot(wavelength_nm, al_eps_imag, \'m-\', linewidth=2)\nax4.axvline(x=800, color=\'orange\', linestyle=\':\', alpha=0.7, label=\'Interband ~800nm\')\nax4.set_xlabel(\'Wavelength (nm)\')\nax4.set_ylabel(\'Îµ_imag\')\nax4.set_title(\'Aluminum - Imaginary Permittivity\')\nax4.grid(True, alpha=0.3)\nax4.set_xlim(wl_min_nm, wl_max_nm)\nax4.legend()\n\nsource_text = \'Palik tabulated data\' if al_loaded else \'Drude model (FALLBACK)\'\nfig.suptitle(f\'Stage 0 â€“ Aluminum Optical Constants â€“ Source: {source_text}\', fontsize=14)\nplt.tight_layout()\nplt.savefig(\'stage0_fig2a_aluminum_optical_constants.png\', dpi=200, bbox_inches=\'tight\')\nplt.close()\nprint("âœ“ Saved: stage0_fig2a_aluminum_optical_constants.png")\n\n# â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\n# PLOT 2: TDBC ABSORPTION SPECTRUM\n# â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\n\nfig, axes = plt.subplots(1, 2, figsize=(14, 5))\n\n# Absorption vs energy (like Fig 2a)\nax1 = axes[0]\nax1.plot(energy_eV, tdbc_absorption_norm, \'b-\', linewidth=2, label=\'TDBC Lorentzian\')\nax1.axvline(x=2.1, color=\'r\', linestyle=\'--\', alpha=0.7, label=\'Target: 2.1 eV\')\nax1.axvline(x=peak_energy_eV, color=\'g\', linestyle=\':\', alpha=0.7, label=f\'Computed: {peak_energy_eV:.3f} eV\')\n\n# Mark FWHM\nax1.axhline(y=0.5, color=\'orange\', linestyle=\':\', alpha=0.5)\nax1.axvline(x=energy_left, color=\'orange\', linestyle=\':\', alpha=0.5)\nax1.axvline(x=energy_right, color=\'orange\', linestyle=\':\', alpha=0.5)\n\nax1.set_xlabel(\'Energy (eV)\')\nax1.set_ylabel(\'Normalized Absorption (a.u.)\')\nax1.set_title(f\'TDBC Absorption â€“ Peak: {peak_energy_eV:.3f} eV, FWHM: {computed_fwhm_eV:.4f} eV\')\nax1.set_xlim(1.5, 3.0)\nax1.set_ylim(0, 1.1)\nax1.legend()\nax1.grid(True, alpha=0.3)\nax1.invert_xaxis()  # Higher energy on left (like wavelength convention)\n\n# Absorption vs wavelength\nax2 = axes[1]\nax2.plot(wavelength_nm, tdbc_absorption_norm, \'b-\', linewidth=2)\nax2.axvline(x=590, color=\'r\', linestyle=\'--\', alpha=0.7, label=\'Target: ~590 nm\')\nax2.axvline(x=peak_wavelength_nm, color=\'g\', linestyle=\':\', alpha=0.7, label=f\'Computed: {peak_wavelength_nm:.1f} nm\')\nax2.set_xlabel(\'Wavelength (nm)\')\nax2.set_ylabel(\'Normalized Absorption (a.u.)\')\nax2.set_title(\'TDBC Absorption vs Wavelength\')\nax2.set_xlim(wl_min_nm, wl_max_nm)\nax2.set_ylim(0, 1.1)\nax2.legend()\nax2.grid(True, alpha=0.3)\n\nfig.suptitle(f\'Stage 0 â€“ TDBC J-aggregate Absorption â€“ Target: Fig. 2a\', fontsize=14)\nplt.tight_layout()\nplt.savefig(\'stage0_fig2a_TDBC_absorption.png\', dpi=200, bbox_inches=\'tight\')\nplt.close()\nprint("âœ“ Saved: stage0_fig2a_TDBC_absorption.png")\n\n# â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\n# PLOT 3: COMBINED VALIDATION SUMMARY\n# â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\n\nfig = plt.figure(figsize=(16, 12))\n\n# Al permittivity (top left)\nax1 = fig.add_subplot(2, 2, 1)\nax1.plot(wavelength_nm, al_eps_real, \'b-\', linewidth=2, label=\'Re(Îµ)\')\nax1.plot(wavelength_nm, al_eps_imag, \'r-\', linewidth=2, label=\'Im(Îµ)\')\nax1.axhline(y=0, color=\'k\', linestyle=\'--\', alpha=0.5)\nax1.set_xlabel(\'Wavelength (nm)\')\nax1.set_ylabel(\'Permittivity\')\nax1.set_title(f\'Aluminum Permittivity\\n(Source: {source_text})\')\nax1.legend()\nax1.grid(True, alpha=0.3)\nax1.set_xlim(wl_min_nm, wl_max_nm)\n\n# TDBC absorption (top right)\nax2 = fig.add_subplot(2, 2, 2)\nax2.plot(energy_eV, tdbc_absorption_norm, \'b-\', linewidth=2)\nax2.axvline(x=2.1, color=\'r\', linestyle=\'--\', linewidth=2, alpha=0.7, label=\'Paper target: 2.1 eV\')\nax2.fill_between([2.1-0.033, 2.1+0.033], 0, 1.1, color=\'r\', alpha=0.1, label=\'FWHM=0.066 eV\')\nax2.set_xlabel(\'Energy (eV)\')\nax2.set_ylabel(\'Normalized Absorption\')\nax2.set_title(f\'TDBC Absorption (using FWHM={paper_fwhm_eV} eV from paper)\')\nax2.set_xlim(1.8, 2.5)\nax2.set_ylim(0, 1.1)\nax2.legend()\nax2.grid(True, alpha=0.3)\n\n# TDBC permittivity (bottom left)\nax3 = fig.add_subplot(2, 2, 3)\nax3.plot(energy_eV, tdbc_eps_real, \'b-\', linewidth=2, label=\'Re(Îµ)\')\nax3.plot(energy_eV, tdbc_eps_imag, \'r-\', linewidth=2, label=\'Im(Îµ)\')\nax3.axvline(x=omega_0_eV, color=\'g\', linestyle=\':\', alpha=0.7, label=f\'Ï‰â‚€={omega_0_eV:.2f} eV\')\nax3.set_xlabel(\'Energy (eV)\')\nax3.set_ylabel(\'Permittivity\')\nax3.set_title(\'TDBC Lorentzian Permittivity\')\nax3.legend()\nax3.grid(True, alpha=0.3)\nax3.set_xlim(1.5, 3.0)\n\n# Validation summary (bottom right)\nax4 = fig.add_subplot(2, 2, 4)\nax4.axis(\'off\')\n\nvalidation_text = f"""\nMATERIAL VALIDATION SUMMARY\nâ•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\n\nALUMINUM (Palik):\n  Source: {source_text}\n  Re(Îµ) < 0 in visible: {\'âœ“ PASS\' if al_plasmonic else \'âœ— FAIL\'}\n  At 500 nm: n={al_n_interp[idx_500]:.3f}, k={al_k_interp[idx_500]:.3f}\n  At 800 nm: Îµ_real={al_eps_real[idx_800]:.1f}, Îµ_imag={al_eps_imag[idx_800]:.1f}\n\nTDBC J-AGGREGATE (Lorentzian):\n  Parameters used:\n    Îµ_âˆ = {eps_inf_tdbc}\n    Ï‰â‚€ = {omega_0_eV:.3f} eV ({1239.84/omega_0_eV:.1f} nm)\n    Î³ = {gamma_eV:.4f} eV (from paper\'s FWHM={paper_fwhm_eV} eV)\n    f = {f_osc}\n  \n  Peak position: {peak_energy_eV:.3f} eV (target: 2.1 eV)\n    Error: {peak_error_pct:.1f}% {\'âœ“ PASS\' if peak_ok else \'âœ— FAIL\'} (threshold: 5%)\n  \n  Linewidth (FWHM): {computed_fwhm_eV:.4f} eV (target: {paper_fwhm_eV} eV)\n    Error: {fwhm_error_pct:.1f}% {\'âœ“ PASS\' if fwhm_ok else \'âœ— FAIL\'} (threshold: 30%)\n\nREVISION NOTES:\n  Previous code used Î³X={2.45e13:.2e} rad/s from paper â†’ FWHMâ‰ˆ0.016 eV\n  Paper states linewidth ~0.066 eV â†’ now using this to back-calculate Î³\n  This resolves the 78% linewidth discrepancy from previous run.\n\nâ•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\nGenerated: {datetime.now().strftime(\'%Y-%m-%d %H:%M:%S\')}\n"""\n\nax4.text(0.05, 0.95, validation_text, transform=ax4.transAxes, fontsize=10,\n         verticalalignment=\'top\', fontfamily=\'monospace\',\n         bbox=dict(boxstyle=\'round\', facecolor=\'wheat\', alpha=0.5))\n\nfig.suptitle(\'Stage 0 â€“ Materials Validation Combined â€“ Target: Fig. 2a\', fontsize=14, fontweight=\'bold\')\nplt.tight_layout()\nplt.savefig(\'stage0_fig2a_materials_validation_combined.png\', dpi=200, bbox_inches=\'tight\')\nplt.close()\nprint("âœ“ Saved: stage0_fig2a_materials_validation_combined.png")\n\n# â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\n# FINAL SUMMARY\n# â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\n\nend_time = datetime.now()\nprint(f"\\n=== Stage 0 Complete ===")\nprint(f"Finished at: {end_time.strftime(\'%Y-%m-%d %H:%M:%S\')}")\n\noutput_files = [\n    \'stage0_aluminum_optical_constants.csv\',\n    \'stage0_TDBC_lorentzian.csv\',\n    \'stage0_fig2a_aluminum_optical_constants.png\',\n    \'stage0_fig2a_TDBC_absorption.png\',\n    \'stage0_fig2a_materials_validation_combined.png\'\n]\nprint(f"Output files: {output_files}")\n\n# Overall validation status\nal_valid = al_plasmonic\ntdbc_valid = peak_ok and fwhm_ok\noverall_valid = al_valid and tdbc_valid\n\nprint(f"\\nValidation status:")\nprint(f"  Aluminum: {\'âœ“ PASS\' if al_valid else \'âš  CHECK\'}")\nprint(f"  TDBC: {\'âœ“ PASS\' if tdbc_valid else \'âš  CHECK\'}")\nprint(f"  Overall: {\'âœ“ READY FOR STAGE 1\' if overall_valid else \'âš  REVIEW REQUIRED\'}")\n\n# â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\n# REPROLAB RESULT SUMMARY (MANDATORY - DO NOT REMOVE)\n# â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\n\nresult_summary = {\n    "status": "completed" if overall_valid else "review_required",\n    "stage_id": "stage0_material_validation